#include <iostream>
#include <math.h>
using namespace std;

int main(){
    float a = sqrt(15);//menghitung akar kuadrat
    float b = cbrt(27);//menghitung akar kubik
    float c = pow(4.334, 2.231);//menghitung pangkat
    float d = sin(30);//menghitung sin
    float e = cos(60);//menghitung cos
    float f = tan(90);//menghitung tan
    float g = round(d);//membulatkan
    
    cout<<a<<endl;
    cout<<b<<endl;
    cout<<c<<endl;
    cout<<d<<endl;
    cout<<e<<endl;
    cout<<f<<endl;
    cout<<g<<endl;
    
    return 0;
}
